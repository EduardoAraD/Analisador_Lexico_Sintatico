PROGRAM PZIM(id, INTEGER);

VAR ano: INTEGER;
    anoatual: INTEGER;
    soma: INTEGER;
BEGIN
    writeln(	'O ano em que você nasceu');
    writeln;
    writeln;
    writeln;
    writeln ('Quantos anos você fez ou irá fazer no ano atual?');
    readln (anoatual);
    soma := anoatual - ano;
    writeln('Você nasceu em', soma);
    writeln;
    writeln;
    writeln;
    writeln
END.