PROGRAM PZIM;

VAR ano: INTEGER;
	anoatual: (*comentario*) INTEGER;
	soma: INTEGER;	
BEGIN
soma := 10..7
soma := 10e++12
soma := 10.7# 
   WRITELN (	'O ano em que você nasceu	');
   WHITELN;
   WRITELN;
   WRITELN;
   WRITELN ('Quantos anos você fez ou irá fazer no ano atual?');
